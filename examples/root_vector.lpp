programa vetorraiz
var 
	vetor_a: conjunto[1..15] de inteiro
	vetor_b: conjunto[1..15] de inteiro
	contador: inteiro
início
	para contador de 1 até 15 passo 1 faça
		escreva "insira o valor"+contador
		leia vetor_a[contador]
	fim_para
	para contador de 1 até 15 passo 1 faça
		vetor_b[contador] ← vetor_a[contador] ↑ (1/2)
	fim_para
fim
