programa vetora
var 
	vetor_a: conjunto[1..5] de inteiro
	vetor_b: conjunto[1..5] de inteiro
	contador: inteiro
início
	para contador de 1 até 5 passo 1 faça
		escreva "insira o valor" + contador
		leia vetor_a[contador]
	fim_para
	para contador de 1 até 5 passo 1 faça
		vetor_b[contador] ← vetor_a[contador]
	fim_para
fim 
