programa vetormultiplica
var 
	vetor_a: conjunto[1..10] de inteiro
	vetor_b: conjunto[1..10] de inteiro
	contador: inteiro
início
	para contador de 1 até 10 passo 1 faça
		escreva "insira o valor"+contador
		leia vetor_a[contador]
	fim_para
	para contador de 1 até 10 passo 1 faça
		vetor_b[contador] ← vetor_a[contador] * contador
	fim_para
fim
